<mujoco model="hopper_rev08">
  <compiler inertiafromgeom='false' angle="radian"/>
  <option integrator="Euler" timestep="0.001"/>
  <size njmax="500" nconmax="100"/>

  <asset>
    <mesh name="base_link" file="base_link.stl"/>
    <mesh name="Link 0" file="Link 0.stl"/>
    <mesh name="Link 1" file="Link 1.stl"/>
    <mesh name="Link 2" file="Link 2.stl"/>
    <mesh name="Link 3" file="Link 3.stl"/>
    <mesh name="rw0" file="rw0.stl"/>
    <mesh name="rw1" file="rw1.stl"/>
    <mesh name="rwz" file="rwz.stl"/>

    <texture builtin="gradient" height="100" rgb1="1 1 1" rgb2="0 0 0" type="skybox" width="100"/>
    <texture builtin="flat" height="1278" mark="cross" markrgb="1 1 1" name="texgeom" random="0.01" rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" type="cube" width="127"/>
    <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
    <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>
    <material name="geom" texture="texgeom" texuniform="true"/>
  </asset>
  <worldbody>
    <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
    <geom conaffinity="1" condim="3" material="MatPlane" name="floor" pos="0 0 0" rgba="0.8 0.9 0.8 1" size="40 40 40" type="plane"/>

    <body name="base_link" pos="0 0 0.7" quat="1 0 0 0">
      <site name='imu' size='0.01' pos='0 0 0'/>
      <inertial pos="-0.00332091752229416 -4.21471772738262E-05 0.0145280493491318" quat="1 0 0 0" mass="4.3805" diaginertia="0.03370055 0.01701751 0.03500408"/>
      <joint type="free" stiffness="0" damping="0" frictionloss="0" armature="0"/>
      <geom type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="0.898039 0.917647 0.929412 1" mesh="base_link"/>
      <geom type="mesh" rgba="0.898039 0.917647 0.929412 1" mesh="base_link"/>
      <body name="Link 0" pos="0 0 0" quat="0.965926 0 0.258819 0">
        <site name="tau0"/>
        <inertial pos="0.00532255 0.0312919 0.000492239" quat="0.447264 0.448457 0.571984 0.521224" mass="0.586955" diaginertia="0.000856436 0.000632559 0.000405846"/>
        <joint name="Joint 0" pos="0 0 0" axis="0 -1 0" limited="true" range="-3.14 3.14"/>
        <geom type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="0.898039 0.917647 0.929412 1" mesh="Link 0"/>
        <geom type="mesh" rgba="0.898039 0.917647 0.929412 1" mesh="Link 0"/>
        <body name="Link 1" pos="0.1 0 0" quat="0.512777 0 0.858522 0">
          <inertial pos="0.133102 0.00662597 -1.77402e-09" quat="0.00128953 0.707106 -0.00128953 0.707106" mass="0.0631474" diaginertia="0.00074371 0.00074327 3.00015e-06"/>
          <joint name="Joint 1" pos="0 0 0" axis="0 -1 0" limited="true" range="-0.7597 1.8295"/>
          <geom type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="0.776471 0.756863 0.737255 1" mesh="Link 1"/>
          <geom type="mesh" rgba="0.776471 0.756863 0.737255 1" mesh="Link 1"/>
        </body>
      </body>
      <body name="Link 2" pos="0 0 0" quat="0.273102 0 0.961985 0">
        <site name="tau1"/>
        <inertial pos="0.0181413 -0.0300645 -0.000292456" quat="0.538041 0.513174 0.47916 0.466444" mass="0.619414" diaginertia="0.00283382 0.00262201 0.000411756"/>
        <joint name="Joint 2" pos="0 0 0" axis="0 -1 0" limited="true" range="-3.14 3.14"/>
        <geom type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="0.898039 0.917647 0.929412 1" mesh="Link 2"/>
        <geom type="mesh" rgba="0.898039 0.917647 0.929412 1" mesh="Link 2"/>
        <body name="Link 3" pos="0.27 0 0" quat="0.512777 0 -0.858522 0">
          <site name='contact_sensor' size='0.02' pos='.27 0 -.0205'/>
          <inertial pos="0.101438 -0.00250998 -0.0148533" quat="0.595716 0.401476 0.567266 0.402677" mass="0.124858" diaginertia="0.00135755 0.00135081 1.98956e-05"/>
          <joint name="Joint 3" pos="0 0 0" axis="0 -1 0" limited="true" range="-1.8295 0.7597"/>
          <geom type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="1 1 1 1" mesh="Link 3"/>
          <geom type="mesh" rgba="1 1 1 1" mesh="Link 3"/>
        </body>
      </body>
      <body name="rw0" pos="0.0565685 -0.137196 0" quat="0 0.92388 -0.382683 0">
        <site name="tau2"/>
        <inertial pos="-0.0115496 -6.5649e-10 1.34792e-09" quat="0 0.92388 0 0.382683" mass="0.712387" diaginertia="0.00364147 0.00190084 0.00190027"/>
        <joint name="joint_rw0" pos="0 0 0" axis="-1 0 0" limited="false"/>
        <geom type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="0.647059 0.619608 0.588235 1" mesh="rw0"/>
        <geom type="mesh" rgba="0.647059 0.619608 0.588235 1" mesh="rw0"/>
      </body>
      <body name="rw1" pos="0.0565685 0.137196 0" quat="0.92388 0 0 0.382683">
        <site name="tau3"/>
        <inertial pos="-0.0115498 6.56414e-10 -1.34804e-09" quat="0 0.92388 0 0.382683" mass="0.712387" diaginertia="0.00364147 0.00190084 0.00190027"/>
        <joint name="joint_rw1" pos="0 0 0" axis="-1 0 0" limited="false"/>
        <geom type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="0.647059 0.619608 0.588235 1" mesh="rw1"/>
        <geom type="mesh" rgba="0.647059 0.619608 0.588235 1" mesh="rw1"/>
      </body>
      <body name="rwz" pos="-0.076 0 0.0835" quat="0 0 0 -1">
        <site name="tau4"/>
        <inertial pos="1.51931e-09 -2.24012e-09 -0.00885752" quat="0.707107 0 0 0.707107" mass="0.356354" diaginertia="0.0008373 0.00043511 0.00043511"/>
        <joint name="joint_rwz" pos="0 0 0" axis="0 0 -1" limited="false"/>
        <geom type="mesh" contype="0" conaffinity="0" group="1" density="0" rgba="0.647059 0.619608 0.588235 1" mesh="rwz"/>
        <geom type="mesh" rgba="0.647059 0.619608 0.588235 1" mesh="rwz"/>
      </body>
    </body>
  </worldbody>

  <equality>
		<connect anchor="0.27 0 0" body1="Link 1" body2="Link 3" name="equality_constraint" solimp="30 3 0.001 0.5 2"/>
	</equality>

  <actuator>
    <motor joint='Joint 0' name='q0' gear="7" forcelimited="true" forcerange="-50 50" ctrllimited="true" ctrlrange="-3.14 3.14"/>
	</actuator>
  <actuator>
    <motor joint='Joint 2' name='q2' gear="7" forcelimited="true" forcerange="-50 50" ctrllimited="true" ctrlrange="-3.14 3.14"/>
	</actuator>
  <actuator>
    <motor joint='joint_rw0' name='rw0' gear="1" forcelimited="true" forcerange="-11.24 11.24" ctrllimited="false"/>
	</actuator>
  <actuator>
    <motor joint='joint_rw1' name='rw1' gear="1" forcelimited="true" forcerange="-11.24 11.24" ctrllimited="false"/>
	</actuator>
  <actuator>
    <motor joint='joint_rwz' name='rwz' gear="1" forcelimited="true" forcerange="-4 4" ctrllimited="false"/>
	</actuator>

  <contact>
    <exclude name="exclude_base_0" body1="base_link" body2="Link 0"/>
    <exclude name="exclude_base_2" body1="base_link" body2="Link 2"/>
    <exclude name="exclude_0_2" body1="Link 0" body2="Link 2"/>
    <exclude name="exclude_0_3" body1="Link 0" body2="Link 3"/>
    <exclude name="exclude_1_2" body1="Link 1" body2="Link 2"/>
    <exclude name="exclude_1_3" body1="Link 1" body2="Link 3"/>

    <exclude name="exclude_base_rw0" body1="base_link" body2="rw0"/>
    <exclude name="exclude_base_rw1" body1="base_link" body2="rw1"/>
    <exclude name="exclude_base_rwz" body1="base_link" body2="rwz"/>
    <exclude name="exclude_2_rwz" body1="Link 2" body2="rwz"/>
  </contact>

  <sensor>
    <!-- User parameter is the number of absolute encoder bits -->
    <framepos     name='base-pos' objtype='site' objname='imu'/>
    <framequat    name='base-attitude' objtype='site' objname='imu'/>
    <velocimeter  name='base-linear-velocity' site='imu'/>
    <gyro  name='base-angular-velocity' site='imu'/>
    <touch  name='contact sensor' site='contact_sensor'/>
    <torque  name='tau0' site='tau0'/>
    <torque  name='tau1' site='tau1'/>
    <torque  name='tau2' site='tau2'/>
    <torque  name='tau3' site='tau3'/>
    <torque  name='tau4' site='tau4'/>
  </sensor>

</mujoco>

